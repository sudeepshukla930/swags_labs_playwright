await products.addItemToCart('Sauce Labs Backpack', '$29.99');\r\n    count++ ; \r\n\r\n    await products.validateAddToCartCount(count.toString());\r\n\r\n    await cart.clickCartIcon();\r\n\r\n    await cart.removeFromToCart('Sauce Labs Onesie','7.99');\r\n\r\n    await cart.clickCheckoutButton();\r\n\r\n    await checkoutOne.fillCheckoutDetails(\r\n        excelData[1][0] ,\r\n        excelData[1][1] ,\r\n        excelData[1][2].toString() \r\n        );\r\n\r\n    await checkoutOne.clickContinueButton();\r\n\r\n    await chheckoutTwo.clickFinisheButton();\r\n\r\n\r\n\r\n\r\n\r\n  });\r\n\r\n\r\n  \r\n});\r\n\r\n\r\n\r\n\r\n"],"mappings":";;AACA,IAAAA,KAAA,GAAAC,OAAA;AACA,IAAAC,gBAAA,GAAAD,OAAA;AACA,IAAAE,aAAA,GAAAF,OAAA;AACA,IAAAG,YAAA,GAAAH,OAAA;AACA,IAAAI,wBAAA,GAAAJ,OAAA;AACA,IAAAK,wBAAA,GAAAL,OAAA;AACA,IAAAM,KAAA,GAAAN,OAAA;AACA,IAAAO,UAAA,GAAAP,OAAA;AAGAQ,UAAI,CAACC,QAAQ,CAAC,WAAW,EAAE,MAAM;EAC/B,IAAIC,IAAU;EACd,IAAIC,KAAiB;EACrB,IAAIC,QAAuB;EAC3B,IAAIC,IAAe;EACnB,IAAIC,WAAiC;EACrC,IAAIC,YAAkC;EACtC,IAAIC,SAAmB,GAAG,EAAE;EAE5B,IAAIC,KAAc;EAElBT,UAAI,CAACU,UAAU,CAAC,OAAO;IAACR;EAAI,CAAC,KAAK;IAEhCC,KAAK,GAAG,IAAIQ,uBAAS,CAACT,IAAI,CAAC;IAC3BE,QAAQ,GAAG,IAAIQ,6BAAY,CAACV,IAAI,CAAC;IACjCG,IAAI,GAAG,IAAIQ,qBAAQ,CAACX,IAAI,CAAC;IACzBI,WAAW,GAAG,IAAIQ,4CAAmB,CAACZ,IAAI,CAAC;IAC3CK,YAAY,GAAG,IAAIQ,4CAAmB,CAACb,IAAI,CAAC;IAC5CM,SAAS,GAAG,MAAMQ,sBAAW,CAACC,SAAS,CAACC,eAAS,CAACC,QAAQ,EAAGD,eAAS,CAACE,SAAS,CAAC;IAGjF,MAAMjB,KAAK,CAACkB,aAAa,CAAC,CAAC;IAE3B,MAAMlB,KAAK,CAACmB,eAAe,CAAC,CAAC;IAE7B,MAAMnB,KAAK,CAACoB,kBAAkB,CAAC,CAAC;IAEjC,MAAMpB,KAAK,CAACqB,WAAW,CAAC,CAAC;EAE1B,CAAC,CAAC;EAMF,IAAAxB,UAAI,EAAE,oCAAmC,EAAE,OAAO;IAACE;EAAI,CAAC,KAAK;IAC3D;IACAO,KAAK,GAAG,CAAC;IACT,MAAML,QAAQ,CAACqB,aAAa,CAAC,mBAAmB,EAAC,OAAO,CAAC;IACzDhB,KAAK,EAAE;IAEP,MAAML,QAAQ,CAACqB,aAAa,CAAC,0BAA0B,EAAC,QAAQ,CAAC;IACjEhB,KAAK,EAAE;IAEP,MAAML,QAAQ,CAACqB,aAAa,CAAC,qBAAqB,EAAE,QAAQ,CAAC;IAC7DhB,KAAK,EAAE;IAEP,MAAML,QAAQ,CAACsB,sBAAsB,CAACjB,KAAK,CAACkB,QAAQ,CAAC,CAAC,CAAC;IAEvD,MAAMtB,I