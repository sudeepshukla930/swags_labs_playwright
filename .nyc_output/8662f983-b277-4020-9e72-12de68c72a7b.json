ginLocator","_cartLocator","_checkoutStepOneLocators","_checkoutStepTwoLocators","_data","_excelRead","_commonTestMethods","_logger","test","describe","page","login","products","cart","checkoutOne","chheckoutTwo","excelData","count","beforeAll","browser","newPage","loginPage","productsPage","cartPage","checkoutStepOnePage","checkoutSteptwoPage","ExcelReader","readExcel","userExcel","filePath","sheetName","Logger","info","goToLoginPage","enterCredential","loginButtonClicked","assertLogin","success","CommonMethods","addItemsToCart","name","price","proceedToCheckoutAndFinish","clickCartIcon","removeFromToCart","clickMenuBarOpen","clickYourMenuElement","clickCheckoutButton","clickCancelOneButton","clickContinueShopButton"],"sources":["endToEndTest.spec.ts"],"sourcesContent":["import { test, Page, expect } from '@playwright/test';\r\nimport { productsPage } from '../pageObjects/productsLocator';\r\nimport { loginPage } from '../pageObjects/loginLocator';\r\nimport { cartPage } from '../pageObjects/cartLocator';\r\nimport { checkoutStepOnePage } from '../pageObjects/checkoutStepOneLocators';\r\nimport { checkoutSteptwoPage } from '../pageObjects/checkoutStepTwoLocators';\r\nimport {userExcel} from '../data/data.json';\r\nimport { ExcelReader } from '../utils/excelRead';\r\nimport { CommonMethods } from '../utils/commonTestMethods';\r\nimport { Logger } from '../utils/logger';\r\n\r\n\r\n\r\ntest.describe('End to End test ', () => {\r\n\r\n    let page: Page;\r\n    let login: loginPage\r\n    let products: productsPage\r\n    let cart: cartPage\r\n    let checkoutOne: checkoutStepOnePage\r\n    let chheckoutTwo: checkoutSteptwoPage\r\n    let excelData : any[][] = [];\r\n    let count : number;\r\n\r\n\r\n    test.beforeAll(async ({ browser }) => {\r\n\r\n        page = await browser.newPage();\r\n        login = new loginPage(page);\r\n        products = new productsPage(page);\r\n        cart = new cartPage(page);\r\n        checkoutOne = new checkoutStepOnePage(page);\